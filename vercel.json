{
    "version": 2,
    "builds": [
        {
            "src": "./connection.mjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "rewrites":  [
        {"source": "/(.*)", "destination": "/"}
    ]
  }